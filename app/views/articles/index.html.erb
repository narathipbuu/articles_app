<h1>Articles</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th>Create Date</th>
  </tr>
<% @articles.each do |article| %>
  <tr>
    <th><%= link_to article.title, article_path(article) %></th>    
    <th><%= article.body %></th>    
    <th><%= article.created_at %></th>    
  </tr>
<% end %>  
</table>

<p><%= link_to "Add Article", new_article_path %></p>

